<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application  name="dubbox-consumer" owner="liuhui"/>

    <dubbo:registry protocol="zookeeper" address="${zookeeper.host}" />

    <dubbo:protocol name="dubbo" port="-1" threadpool="cached" threads="200"/>
    <!-- url:指定访问资源 -->
    <dubbo:reference interface="com.example.dubbo.service.HelloService" id="helloService" >
    </dubbo:reference>


</beans>